{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "chat-web",
    "ejected": true
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist/pc",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.pc.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig-app.pc.json",
      "testTsconfig": "tsconfig-app.pc.json",
      "prefix": "app",
      "styles": [
        "pc/styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
